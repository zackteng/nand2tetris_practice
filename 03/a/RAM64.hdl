/**
 * Memory of 64 registers, each 16 bit-wide. Out holds the value
 * stored at the memory location specified by address. If load==1, then 
 * the in value is loaded into the memory location specified by address 
 * (the loaded value will be emitted to out from the next time step onward).
 */

CHIP RAM64 {
    IN in[16], load, address[6];
    OUT out[16];

    PARTS:
    DMux8Way(in=load, sel=address[3..5], a=outa, b=outb, c=outc, d=outd, e=oute, f=outf, g=outg, h=outh);
    RAM8(in=in, load=outa, address=address[0..2], out=routa);
    RAM8(in=in, load=outb, address=address[0..2], out=routb);
    RAM8(in=in, load=outc, address=address[0..2], out=routc);
    RAM8(in=in, load=outd, address=address[0..2], out=routd);
    RAM8(in=in, load=oute, address=address[0..2], out=route);
    RAM8(in=in, load=outf, address=address[0..2], out=routf);
    RAM8(in=in, load=outg, address=address[0..2], out=routg);
    RAM8(in=in, load=outh, address=address[0..2], out=routh);
    Mux8Way16(a=routa, b=routb, c=routc, d=routd, e=route, f=routf, g=routg, h=routh, sel=address[3..5], out=out);
    // Put your code here:
}